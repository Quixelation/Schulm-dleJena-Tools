{
  "manifest_version": 2,
  "version": "0.9.2",
  "name": "SchulmoodleJena Tools",
  "description": "Tools für das Jenaer Schulmoodle",
  "browser_specific_settings": {
    "gecko": {
      "id": "schulmoodlejenatools@example.com",
      "strict_min_version": "42.0",
      "update_url": "https://schulmoodle-jena-tools.vercel.app/updates.json"
    }
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Jenaer Moodle",
    "browser_style": false,
    "default_popup": "static/popup/index.html"
  },
  "icons": {
    "16": "icons/16x16.png",
    "24": "icons/24x24.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "background": {
    "scripts": ["generated/background.js"],
    "persistent": true
  },
  "permissions": ["storage", "tabs", "unlimitedStorage"],
  "author": "Robert Steffen Stündl",
  "content_scripts": [
    {
      "matches": [
        "https://moodle.jsp.jena.de/*",
        "https://moodle.jsp.jena.de/*"
      ],
      "js": ["generated/manager.js", "generated/todo/bundle.js"],
      "css": ["css/global.css", "generated/todo/bundle.css"]
    },
    {
      "matches": [
        "http://moodle.jsp.jena.de/mod/resource/view.php*",
        "https://moodle.jsp.jena.de/mod/resource/view.php*"
      ],
      "js": ["generated/adjustVideo.js"]
    }
  ]
}
